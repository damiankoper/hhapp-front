{
  "name": "hhapp-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "vue-cli-service test:unit",
    "coverage": "npm run test -- --coverage",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:dev:storybook": "docker-compose -f docker-compose.dev.yml run hhapp-front-dev ./node_modules/.bin/start-storybook -p 6006",
    "docker:test": "docker-compose -f docker-compose.dev.yml run hhapp-front-dev npm run coverage",
    "docker:test:watch": "docker-compose -f docker-compose.dev.yml run --rm hhapp-front-dev npm run test -- --watch",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up --build -d"
  },
  "dependencies": {
    "@storybook/addon-centered": "^5.1.11",
    "@storybook/addon-knobs": "^5.1.11",
    "@storybook/theming": "^5.1.11",
    "@types/animejs": "^3.1.0",
    "@types/color": "^3.0.0",
    "@types/hammerjs": "^2.0.36",
    "@types/storybook__addon-centered": "^3.4.1",
    "@types/storybook__vue": "^5.0.2",
    "@vue/cli-plugin-pwa": "^3.10.0",
    "@vue/cli-plugin-typescript": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "animejs": "^3.1.0",
    "color": "^3.1.2",
    "coveralls": "^3.0.6",
    "fork-ts-checker-webpack-plugin": "^1.5.0",
    "hammerjs": "^2.0.8",
    "node-sass": "^4.13.1",
    "register-service-worker": "^1.6.2",
    "reset-css": "^4.0.1",
    "sass-loader": "^7.1.0",
    "typescript": "^3.4.3",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.3",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.0.1",
    "webpack-merge": "^4.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/addons": "^5.1.11",
    "@storybook/vue": "^5.1.11",
    "@types/jest": "^23.1.4",
    "@vue/cli-plugin-unit-jest": "^3.10.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-loader": "^8.0.6",
    "babel-preset-vue": "^2.0.2",
    "ts-jest": "^23.0.0"
  }
}
